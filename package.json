{
  "name": "salt-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite-react-ssg dev",
    "build": "npm run build-prod",
    "build-prod": "tsc && vite-react-ssg build --mode production; npm run robots:prod",
    "build-dev": "tsc && vite-react-ssg build --mode development; npm run robots:dev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "npm run lint -- --fix",
    "preview": "vite preview",
    "postinstall": "patch-package",
    "generate-datocms-types": "graphql-codegen --config codegen.yml",
    "robots:prod": "cp robots/robots.prod.txt dist/robots.txt",
    "robots:dev": "cp robots/robots.dev.txt dist/robots.txt",
    "bundle-analyze": "npx vite-bundle-visualizer -t sunburst"
  },
  "dependencies": {
    "@awesome.me/kit-b6cda292ae": "^1.0.7",
