    "axios": "^1.6.7",
    "axios-auth-refresh": "^3.3.6",
    "firebase": "^10.8.0",
    "js-cookie": "^3.0.5",
    "framer-motion": "^11.0.25",
    "framer-motion-3d": "^11.0.25",
    "jwt-decode": "^4.0.0",
    "lodash.throttle": "^4.1.1",
    "maath": "^0.10.7",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-datocms": "^5.0.3",
